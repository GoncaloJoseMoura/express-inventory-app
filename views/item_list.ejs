<div class="items-list">
  <% if (title !== undefined) {%>
  <h1><%= title %></h1>
  <% }%> <% if (!items.length) {%>
  <h3 style="text-align: center">No items available</h3>
  <% } %> <% items.forEach(item => { %>
  <a href="<%= item.url %>" class="item-card">
    <p><%= item.name %></p>
    <p><%= item.price %> $</p>
    <p><%= item.numberInStock %> in stock</p>
  </a>
  <% }); %>
</div>
